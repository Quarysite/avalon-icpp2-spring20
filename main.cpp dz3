#include<iostream>
#include<random>
using namespace std;



int main()
{
	const int agents = 10;
	int paymentOfagentsInDays[agents][12];
	int varietyofwork;
	for (int i = 0; i < agents; i++)
	{
		for (int j = 0; j < 12; j++)
		{
			varietyofwork = rand() % 10;
			if (varietyofwork % 2 == 0)
				paymentOfagentsInDays[i][j] = rand() % 100000 + 1000;
			else
				paymentOfagentsInDays[i][j] = -1;
		}
	}
	for (int i = 0; i < agents; i++)
	{
		for (int j = 0; j < 12; j++)
		{
			cout << paymentOfagentsInDays[i][j];
			cout << endl;
		}
		cout << endl;
	}


}
